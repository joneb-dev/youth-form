		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-react": "^7.32.2",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.0",
		"prettier": "2.8.7",
		"pretty-quick": "^3.1.3"
	},
	"lint-staged": {
		"*.js": [
			"npm run lint",
			"eslint --cache --fix"
		],
		"*.{js,css,md,html,json}": "npm run format",
		"*.--write": "prettier --write"
	}
